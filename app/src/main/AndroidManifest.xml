<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.zenwsmp.pmwani">


    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MainApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_pmwani"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_pmwani_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PMWANI.NoActionBar">
        <activity
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden"
            android:name=".TransactionListActivity"
            android:exported="true" />
        <activity
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden"
            android:name=".SessionListActivity"
            android:exported="true" />
        <activity
            android:name=".AvailablePlanActivity"
            android:exported="true"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name=".Base_Drawer"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name=".Change_Password_Activity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".DashboardActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ForgotPassword_Activity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.PMWANI.NoActionBar" />
        <activity
            android:name=".EditPersonalDetail_Activity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.PMWANI.NoActionBar" />
        <activity
            android:name=".AddVoucherCode_Activity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.PMWANI.NoActionBar" />
        <activity
            android:name=".MainScreen_Activity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.PMWANI.NoActionBar" />
        <activity
            android:name=".Account_Activity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.PMWANI.NoActionBar" />
        <activity
            android:name=".PreparationSetup_Activity"
            android:screenOrientation="portrait"
            android:theme="@style/custome" />
        <activity
            android:name=".ProfilePicture_Activity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.PMWANI.NoActionBar" />
        <activity
            android:name=".LoginWith2FA_Activity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.PMWANI.NoActionBar" />
        <activity
            android:name=".Setup2FA_Activity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.PMWANI.NoActionBar" />
        <activity
            android:name=".OTP_Activity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.PMWANI.NoActionBar" />
        <activity
            android:name=".RegistrationActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.PMWANI.NoActionBar" />
        <activity
            android:name=".wifi.AccessPointsActivity"
            android:exported="true"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.PMWANI.NoActionBar" />
        <activity
            android:name=".wifi.MainActivity"
            android:exported="true"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.PMWANI.NoActionBar" />
        <activity
            android:name=".SplashActivity"
            android:exported="true"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.PMWANI.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.PMWANI.NoActionBar" />
        <activity
            android:name=".Startup_Activity"
            android:screenOrientation="portrait"
            android:theme="@style/custome" />
        <activity
            android:name=".LanguageSelection_Activity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.PMWANI.NoActionBar" />

        <receiver
            android:name=".MySMSBroadcastReceiver"
            android:exported="true">
            <intent-filter>
                <action android:name="com.google.android.gms.auth.api.phone.SMS_RETRIEVED" />
            </intent-filter>
        </receiver>

        <activity android:name=".activity.RazorPayOrderPlaceActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".activity.RazorPayNetBankingActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".activity.RazorPayUpiActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".activity.RazorPayCreditDebitCardActivity"
            android:screenOrientation="portrait">


        </activity>
        <activity android:name=".activity.RazorPayCustomActivity" />
        <activity android:name=".activity.RazorPayWalletActivity" />
        <activity
            android:name="com.paytm.pgsdk.PaytmPGActivity"
            android:configChanges="keyboardHidden|orientation|keyboard"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme"
            tools:ignore="LockedOrientationActivity" />
    </application>

</manifest>